        <a class="nav-link text-black" asp-controller="Empleados" asp-action="Perfil">
            Mi Perfil
        </a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Empleados" asp-action="Compis">Compis</a>
    </li>
    <li class="nav-link">
        <a class="nav-link text-danger" asp-action="Logout" asp-controller="Managed">
